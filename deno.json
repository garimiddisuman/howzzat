{
  "lint": {
    "rules": {
      "tags": ["recommended"],
      "include": [
        "camelcase",
        "no-unused-vars",
        "no-debugger",
        "no-constant-condition",
        "no-empty-interface",
        "no-var"
      ],
      "exclude": []
    }
  },

  "fmt": {
    "indentWidth": 2,
    "lineWidth": 80,
    "semiColons": true,
    "singleQuote": true
  },
  "imports": {
    "lodash": "npm:lodash@^4.17.21"
  }
}
